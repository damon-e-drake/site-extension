<div class="jumbotron">
  
</div>
<div class="row">
  <div class="col-md-6">
    <h3>Summary</h3>
    <table class="table table-striped">
      <tr>
        <th>Domain</th>
        <th>Docs</th>
      </tr>
      <tbody data-bind="foreach: domains">
        <tr>
          <td data-bind="text: Name"></td>
          <td data-bind="text: Documents"></td>
        </tr>
      </tbody>
    </table>
  </div>
  <div class="col-md-6">
    <h2>Get more libraries</h2>
    <p>NuGet is a free Visual Studio extension that makes it easy to add, remove, and update libraries and tools in Visual Studio projects.</p>
    <p><a class="btn btn-default" href="http://go.microsoft.com/fwlink/?LinkId=301871">Learn more &raquo;</a></p>
  </div>
</div>

@section scripts {
  <script>
    /// <reference path="scripts/_references.js" />
    var dvm = new DashboardViewModel();

    $(document).ready(function () {
      ko.applyBindings(dvm);
    });
  </script>
}